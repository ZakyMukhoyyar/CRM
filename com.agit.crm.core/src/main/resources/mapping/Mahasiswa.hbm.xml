<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-access="field">
    <class mutable="true" name="com.agit.crm.domain.crm.Mahasiswa" table="AGIT_MAHASISWA">
        <!--<cache usage="read-only"/>-->
        <id column="id" name="id">
            <generator class="org.hibernate.id.IdentityGenerator"/>
        </id>
        <property column="idMahasiswa" name="idMahasiswa" not-null="false"/>
        <property column="username" name="username" not-null="false"/>
        <property column="password" name="password" not-null="false"/>
        <property column="namaLengkap" name="namaLengkap" not-null="false"/>
        <property column="email" name="email"/>
        <property column="tanggalLahir" name="tanggalLahir" not-null="false"/>
        <property column="domisili" name="domisili" not-null="false"/>
        <property column="noHP" name="noHP"/>
        <property column="pendidikanType" name="pendidikanType" not-null="false">
            <type name="org.hibernate.type.EnumType">
                <param name="enumClass">com.agit.crm.shared.type.PendidikanType</param>
                <param name="type">12</param>
            </type>
        </property>
        <property column="namaCivitas" name="namaCivitas" not-null="false"/>
        <property column="minat" name="minat" not-null="false"/>
        <property column="ketrampilan1" name="ketrampilan1"/>
        <property column="ketrampilan2" name="ketrampilan2"/>        
        <property column="ketrampilan3" name="ketrampilan3"/>
        <property column="ketrampilan4" name="ketrampilan4"/>
        <property column="ketrampilan5" name="ketrampilan5"/>
        <property column="tingkat1" name="tingkat1"/>
        <property column="tingkat2" name="tingkat2"/>        
        <property column="tingkat3" name="tingkat3"/>
        <property column="tingkat4" name="tingkat4"/>
        <property column="tingkat5" name="tingkat5"/>
        <property column="uploadCV" name="uploadCV"/>
        <list cascade="all" lazy="false" name="lowongans">
            <key column="idMahasiswa" foreign-key="idMahasiswa"/>
            <index column="lowongan_index"/>
            <one-to-many class="com.agit.crm.domain.crm.Lowongan"/>
        </list>
        <property column="createdDate" name="createdDate" not-null="false"/>
        <property column="createdBy" name="createdBy"/>
        <property column="modifiedDate" name="modifiedDate" not-null="false"/>
        <property column="modifiedBy" name="modifiedBy" not-null="false"/>
    </class>
</hibernate-mapping>
